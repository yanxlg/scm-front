        "husky": "^4.2.3",
        "stylelint-config-standard": "^20.0.0"
    },

    "husky": {
        "hooks": {
            "pre-commit": "npm run lint-staged"
        }
    },
    "lint-staged": {
        "**/*.less": "stylelint --syntax less",
        "**/*.{js,jsx,tsx,ts,less,md,json}": [
            "prettier --write",
            "git add"
        ],
        "**/*.{js,jsx}": "npm run lint-staged:js",
        "**/*.{js,ts,tsx}": "npm run lint-staged:js"
    },
    "engines": {
        "node": ">=8.0.0"
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not ie <= 10"
    ],
    "checkFiles": [
        "src/**/*.js*",
        "src/**/*.ts*",
        "src/**/*.less",
        "config/**/*.js*",
        "scripts/**/*.js"
    ]
}
