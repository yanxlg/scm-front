{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "moduleResolution": "node",
        "importHelpers": true,
        "jsx": "react",
        "esModuleInterop": true,
        "sourceMap": true,
        "baseUrl": ".",
        "strict": true,
        "experimentalDecorators": true,
        "resolveJsonModule": true,
        "paths": {
            "@/*": ["src/*"],
            "@@/*": ["src/.umi/*", "src/.umi-production/*"]
        },
        "allowSyntheticDefaultImports": true
    }
}
